<base-title class="mb-7">
  <img
    class="w-7 h-7 md:block hidden"
    src="<%= image_path('icons/feedback-sessions-desktop.png') %>"
  >
  <img
    class="w-7 h-7 block md:hidden"
    src="<%= image_path('icons/feedback-sessions.png') %>"
  >
  Sesiones de Feedback
  <base-button class="flex ml-auto px-2" href="/app/feedback_sessions/new">
    <img
      class="w-4 mr-2"
      src="<%= image_path('icons/add-circle.svg') %>"
    >
    Añadir sesión
  </base-button>
</base-title>

<feedback-sessions
  initial-type="provider"
  :initial-sessions="<%= serialize_resource(@provider_sessions) %>"
></feedback-sessions>
